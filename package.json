{
  "name": "saga-testing",
  "version": "1.0.0",
  "description": "Demo for testing redux-sagas",
  "main": "index.js",
  "engines": {
    "node": "=8"
  },
  "scripts": {
    "postinstall": "NODE_ENV=production ./node_modules/.bin/webpack",
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon -w app index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-core/register --require ignore-styles \"test/**/*.spec.js\"",
    "lint": "NODE_ENV=test ./node_modules/.bin/eslint src test webpack webpack.config.js"
  },
  "author": "Fela Maslen",
  "license": "UNLICENSED",
  "babel": {
    "env": {
      "production": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "ie >= 10"
                ]
              }
            }
          ],
          "stage-0"
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "react",
          "stage-0"
        ],
        "plugins": [],
        "sourceMaps": "inline"
      },
      "test": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "node": "8"
              }
            }
          ],
          "stage-0"
        ]
      }
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "ejs": "^2.5.7",
    "eslint-loader": "^1.9.0",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-saga": "^0.16.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.4.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "react-test-renderer": "^16.1.1",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^3.3.0",
    "redux-test-utils": "^0.2.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.21.0"
  }
}
